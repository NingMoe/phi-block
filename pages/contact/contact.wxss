/* pages/contact/contact.wxss */
.scroll{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}
.box{
  padding: 30rpx;
  box-sizing: border-box;
}
.item{
  height: 72rpx;
  line-height: 72rpx;
  font-size: 28rpx;
}
.item .left{
  width: 40%;
  float: left;
  height: 100%;
  box-sizing: border-box;
  border-bottom: 1px solid #ccc;
  background: #348ce6;
  color: #fff;
  text-align: center;
}
.item .right{
  height: 100%;
  width: 60%;
  float: left;
  text-align: center;
  color: #666;
}
.item .f1{
  background: #f1f1f1;
}
.item .d1{
  background: #f9f9f9;
}
.sale-icon::after{
  content: "\e65a";
}
.hou-icon::after{
  content: "\e610";
}
.address-icon::after{
  content: "\e61b";
}
.title{
  text-align: center;
  padding-top: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  color: #70aeed;
}
.title::before{
  content: '[';
  color: #348ce6;
  position: absolute;
  font-weight: bold;
  font-size: 60rpx;
  left: 220rpx;
}
.title::after{
  content: ']';
  color: #348ce6;
  position: absolute;
  font-weight: bold;
  font-size: 60rpx;
  right: 220rpx;
}

.title .icon{
  margin-right: 20rpx;
  font-size: 60rpx;
}
.msg{
  background: #1c1c1c;
  padding: 30rpx;
  box-sizing: border-box;
}
.msg .msg_input{
  height: 72rpx;
  width: 100%;
  background: #fff;
  margin-bottom: 10rpx;
  box-sizing: border-box;
  padding: 0 15rpx;
}
.msg textarea.msg_input{
  height: 144rpx;
  padding-top: 15rpx;

}
.btn{
  background: #348ce6;
  color: #fff;
}
.map{
  width: 100%;
  height: 600rpx;
}
.address{
  padding: 30rpx;
  box-sizing: border-box;
}
.address .a{
  font-size: 28rpx;
}
.address .b{
  font-size: 24rpx;
  color: #999;
  margin-top: 10rpx;
}