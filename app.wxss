.icon{
  font-family: 'iconfont';
  font-style: normal;
}
.fill-img{
  height:100%;width:100%;vertical-align:top;
}
.ellipsis{
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}